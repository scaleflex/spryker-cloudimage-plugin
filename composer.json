{
    "name": "evl/cloudimage",
    "description": "Spryker package to integrate Cloudimage",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Evl\\": "src/Evl/"
        }
    },
    "authors": [
        {
            "name": "Ellen Wenzel"
        },
        {
            "name": "Linas Linartas"
        },
        {
            "name": "Vincent Christophe"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "spryker/kernel": "^3.30.0",
        "spryker/twig-extension": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.17.7"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
